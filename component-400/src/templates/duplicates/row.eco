<% if @input: %>
    <td rowspan="<%= @rowspan %>" class="<%= @class %>">
        <%- @input %>
        <% if @continuing: %><em>cont.</em><% end %>
    </td>
<% end %>
<td>
    <a><%- @matched %></a>
    <span class="help-flyout">i</span>
</td>
<% if @selected: %>
    <td><span class="tiny secondary button">Remove</span></td>
<% else: %>
    <td><span class="tiny success button">Add</span></td>
<% end %>