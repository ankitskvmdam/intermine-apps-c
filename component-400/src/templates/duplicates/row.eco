<% if @provided: %>
    <td rowspan="<%= @rowspan %>"><%- @provided %></td>
<% end %>
<td>
    <a><%- @matched %></a>
    <span class="help-flyout"></span>
</td>
<% if @selected: %>
    <td><span class="tiny secondary button">Remove</span></td>
<% else: %>
    <td><span class="tiny success button">Add</span></td>
<% end %>