<% if @input: %>
    <% if @continuing: %>
        <td rowspan="<%= @rowspan %>" class="<%= @class %>"><%- @input %> <em>cont.</em></td>
    <% else: %>
        <td rowspan="<%= @rowspan %>" class="<%= @class %>"><%- @input %></td>
    <% end %>
<% end %>
<td>
    <a><%- @matched %></a>
    <span class="help-flyout"></span>
</td>